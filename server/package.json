{
  "name": "spotify97-server",
  "version": "1.2.0.0",
  "description": "Facilitates communication between Spotify API and local (retro) client machines",
  "main": "src/app.js",
  "bin": "dist/js/app.js",
  "author": "queenkjuul",
  "homepage": "https://github.com/queenkjuul/spotify97#readme",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/queenkjuul/spotify97/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/queenkjuul/spotify97.git"
  },
  "keywords": [
    "spotify",
    "retro",
    "retrocomputing",
    "windows98",
    "windows95",
    ".net"
  ],
  "scripts": {
    "build:js": "npm run clean:js && npx tsc && cpy .env.example --rename=.env dist/js/",
    "build:linux:x64": "npm run build:js && mkdirp dist/linux/x64 && pkg -t latest-linux-x64 -o dist/linux/x64/spotify97-server dist/js/app.js && cpy .env.example --rename=.env dist/linux/x64/",
    "build:linux:arm64": "npm run build:js && mkdirp dist/linux/arm64 && pkg -t latest-linux-arm64 -o dist/linux/arm64/spotify97-server dist/js/app.js && cpy .env.example --rename=.env dist/linux/arm64/",
    "build:macos": "rimraf dist/macos && npm run build:js && mkdirp dist/macos/x64 && pkg -t latest-macos-x64 -o dist/macos/x64/spotify97-server dist/js/app.js && cpy .env.example --rename=.env dist/macos/x64/",
    "build:all": "npm run build:js && npm run build:linux:x64; npm run build:linux:arm64; npm run build:macos",
    "build:windows": "rimraf dist/win && mkdirp dist/win && exe && cpy .env.example --rename=.env dist/win/",
    "build:docker": "npm run build:js && docker build . -t spotify97-server",
    "start": "node dist/js/app.js",
    "serve": "npm run build:js && npm run start",
    "dev": "nodemon src/app.ts",
    "clean:all": "rimraf dist && rimraf state.json",
    "clean:js": "rimraf dist/js",
    "clean:windows": "rimraf dist/win"
  },
  "dependencies": {
    "accepts": "^1.3.8",
    "array-flatten": "^1.1.1",
    "body-parser": "^1.20.3",
    "bytes": "^3.1.2",
    "call-bind": "^1.0.7",
    "content-disposition": "^0.5.4",
    "content-type": "^1.0.5",
    "cookie": "^0.7.1",
    "cookie-signature": "^1.0.6",
    "debug": "^2.6.9",
    "define-data-property": "^1.1.4",
    "depd": "^2.0.0",
    "destroy": "^1.2.0",
    "dotenv": "^16.4.5",
    "ee-first": "^1.1.1",
    "encodeurl": "^2.0.0",
    "es-define-property": "^1.0.0",
    "es-errors": "^1.3.0",
    "escape-html": "^1.0.3",
    "etag": "^1.8.1",
    "express": "^4.21.1",
    "finalhandler": "^1.3.1",
    "forwarded": "^0.2.0",
    "fresh": "^0.5.2",
    "function-bind": "^1.1.2",
    "get-intrinsic": "^1.2.4",
    "gopd": "^1.0.1",
    "has-property-descriptors": "^1.0.2",
    "has-proto": "^1.0.3",
    "has-symbols": "^1.0.3",
    "hasown": "^2.0.2",
    "http-errors": "^2.0.0",
    "iconv-lite": "^0.4.24",
    "inherits": "^2.0.4",
    "ipaddr.js": "^1.9.1",
    "jimp": "^1.6.0",
    "media-typer": "^0.3.0",
    "merge-descriptors": "^1.0.3",
    "methods": "^1.1.2",
    "mime": "^1.6.0",
    "mime-db": "^1.52.0",
    "mime-types": "^2.1.35",
    "ms": "^2.0.0",
    "negotiator": "^0.6.3",
    "object-inspect": "^1.13.2",
    "on-finished": "^2.4.1",
    "parseurl": "^1.3.3",
    "path-to-regexp": "^0.1.10",
    "proxy-addr": "^2.0.7",
    "qs": "^6.13.0",
    "range-parser": "^1.2.1",
    "raw-body": "^2.5.2",
    "safe-buffer": "^5.2.1",
    "safer-buffer": "^2.1.2",
    "send": "^0.19.0",
    "serve-static": "^1.16.2",
    "set-function-length": "^1.2.2",
    "setprototypeof": "^1.2.0",
    "side-channel": "^1.0.6",
    "statuses": "^2.0.1",
    "toidentifier": "^1.0.1",
    "type-is": "^1.6.18",
    "undici-types": "^6.19.8",
    "unpipe": "^1.0.0",
    "utils-merge": "^1.0.1",
    "vary": "^1.1.2"
  },
  "devDependencies": {
    "@angablue/exe": "^3.2.3",
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.5",
    "cpy-cli": "^5.0.0",
    "mkdirp": "^3.0.1",
    "nodemon": "^3.1.7",
    "pkg": "^5.8.1",
    "rimraf": "^5.0.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "nodemonConfig": {
    "ignore": [
      "state.json",
      "routes.json"
    ]
  }
}
